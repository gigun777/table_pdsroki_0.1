{
  "name": "@dilovodstvo/subrows",
  "version": "0.1.0",
  "description": "Independent pure-logic subrows core for table engines",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
